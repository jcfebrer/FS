<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
    <appSettings>
      <add key="LogFile" value="FSTrace.log"/>
    </appSettings>
    <connectionStrings>
      <add name="FSConnection" connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Persist Security Info=False;Data Source={app_path}\Data\fsgestion.mdb" providerName="System.Data.OleDb"/>
    </connectionStrings>
  
    <system.diagnostics>
        <sources>
            <!-- En esta secci贸n se define la configuraci贸n del registro para My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Quite los comentarios de la secci贸n posterior para escribir en el registro de eventos de la aplicaci贸n -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information"/>
            <!--0- desactivado, 1- error, 2- warning, 3- info, 4- verbose -->
            <add name="LogLevel" value="3"/>
        </switches>
    </system.diagnostics>
