<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:FSPortalWPF="clr-namespace:FSPortalWPF"
	x:Class="FSPortalWPF.modContenido"
	x:Name="UserControl"
	Width="640" Height="480" xmlns:FSPortalWPF_Controles="clr-namespace:FSPortalWPF.Controles">

	<Grid x:Name="LayoutRoot">
		<TabControl IsSynchronizedWithCurrentItem="True" Width="Auto" Margin="0,0,0,0">
			<TabItem Header="Registro">
				<Grid>
					<FSPortalWPF:modRegistro Width="Auto" Height="Auto"/>
				</Grid>
			</TabItem>
			<TabItem Header="TabItem">
				<FSPortalWPF:editor Width="Auto" Height="Auto"/>
			</TabItem>
			<TabItem Header="TabItem">
				<Grid>
					<FSPortalWPF_Controles:modCalculadora Margin="0,0,0,0"/>
				</Grid>
			</TabItem>
		</TabControl>
	</Grid>
</UserControl>